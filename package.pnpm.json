{
  "name": "frontend-snippets",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro build --verbose",
    "check": "astro check",
    "dev": "astro dev",
    "info": "astro info",
    "preview": "astro preview --verbose",
    "debug": "DEBUG=true pnpm run dev",
    "docs": "jsdoc -c ./jsdoc.config.json -r",
    "fix:js": "eslint . --fix --ext .js,.mjs,.cjs",
    "fix:md": "markdownlint \"**/*.md\" --fix",
    "lint:js": "eslint . --ext .js,.mjs,.cjs",
    "lint:md": "markdownlint \"**/*.md\"",
    "prettier": "prettier --check .",
    "prettier:write": "prettier --write .",
    "postcss": "postcss ./public/snippets/**/*.css --replace --config postcss.config.cjs",
    "start": "pnpm run dev",
    "sync": "astro sync",
    "test": "pnpm run vitest:test",
    "test:all": "npm-run-all vitest:test playwright:test cypress:test",
    "cypress:ci": "start-server-and-test start http://localhost:4321 cypress:test",
    "cypress:ui": "pnpm exec cypress open",
    "cypress:debug": "DEBUG=cypress:* pnpm run cypress:test",
    "cypress:test": "pnpm exec cypress run",
    "playwright:ci": "start-server-and-test start http://localhost:4321 playwright:test",
    "playwright:codegen": "pnpm exec playwright codegen",
    "playwright:test": "pnpm exec playwright test",
    "playwright:debug": "pnpm exec playwright test --debug",
    "playwright:ui": "pnpm exec playwright test --ui",
    "playwright:chrome": "pnpm exec playwright test --project=chromium",
    "playwright:report": "pnpm exec playwright show-report",
    "vitest:test": "vitest"
  },
  "dependencies": {
    "@astrojs/check": "~0.2.0",
    "@astrojs/mdx": "~1.1.0",
    "astro": "~3.1.0",
    "libphonenumber-js": "~1.10.44",
    "typescript": "~5.2.2"
  },
  "devDependencies": {
    "@playwright/test": "~1.38.1",
    "@types/node": "~20.6.2",
    "@vitest/coverage-v8": "~0.34.6",
    "clean-jsdoc-theme": "~4.2.10",
    "css-declaration-sorter": "~7.1.1",
    "cypress": "~13.3.0",
    "dotenv": "~16.3.1",
    "eslint": "~8.49.0",
    "eslint-config-prettier": "~9.0.0",
    "jsdoc": "~4.0.2",
    "markdownlint": "~0.31.1",
    "markdownlint-cli": "~0.36.0",
    "mochawesome": "~7.1.3",
    "mochawesome-merge": "~4.3.0",
    "mochawesome-report-generator": "~6.2.0",
    "npm-run-all": "~4.1.5",
    "open-cli": "~7.2.0",
    "postcss": "~8.4.31",
    "postcss-cli": "~10.1.0",
    "prettier": "~3.0.3",
    "prettier-plugin-astro": "~0.12.0",
    "prettier-plugin-css-order": "~2.0.1",
    "prettier-plugin-packagejson": "~2.4.5",
    "sentences-per-line": "~0.2.1",
    "start-server-and-test": "~2.0.1",
    "ts-node": "~10.9.1",
    "vitest": "~0.34.6",
    "zod": "~3.22.4"
  },
  "packageManager": "pnpm@8.7.4"
}
